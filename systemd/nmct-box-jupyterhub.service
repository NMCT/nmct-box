[Unit]
Description=Jupyter notebook for NMCT box
After=network.target

[Service]
User=nmct
Group=www-data
WorkingDirectory=${NMCT_HOME}/Notebooks
Environment="PATH=${NMCT_HOME}/env/bin"
ExecStart=${NMCT_HOME}/env/bin/jupyterhub --ip=0.0.0.0 --port=2003 --base-url=/hub

[Install]
WantedBy=multi-user.target
